{
  "name": "disaster-management-system",
  "version": "1.0.0",
  "description": "A comprehensive disaster management and alert system for landslides and floods",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "disaster",
    "management",
    "alerts",
    "landslides",
    "floods"
  ],
  "author": "Disaster Management Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poshikaraj324-crypto/Disaster-Management.git"
  },
  "bugs": {
    "url": "https://github.com/poshikaraj324-crypto/Disaster-Management/issues"
  },
  "homepage": "https://github.com/poshikaraj324-crypto/Disaster-Management#readme"
}
